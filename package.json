{
  "name": "dashboard-survey",
  "version": "1.0.0",
  "description": "Dashboard application for visualizing survey data with MongoDB integration and API endpoint for external submissions",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
    "build": "cd client && npm run build",
    "vercel-build": "npm install && cd client && npm install && npm run build"
  },
  "keywords": [
    "react",
    "mongodb",
    "survey",
    "dashboard"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^7.5.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.1"
  }
}